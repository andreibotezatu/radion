  "dependencies": {
    "autoprefixer": "latest",
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "expect": "^1.12.2",
    "express": "latest",
    "express-jwt": "^3.4.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fastclick": "^1.0.6",
    "fbjs": "0.8.3",
    "history": "^3.0.0",
